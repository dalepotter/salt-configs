# Define UNIX username and home directory details for the Dashboard installation
# In practice these all take the same data, since these are depenencies needed for the Dashboard to run

registry-refresher:
  unix-user-name: dashboard
  unix-user-home-directory: /home/dashboard

stats:
  unix-user-name: dashboard
  unix-user-home-directory: /home/dashboard

dashboard:
  unix-user-name: dashboard
  unix-user-home-directory: /home/dashboard
